#include "main.h"
#include <stdio.h>
/**
 *print_to_98 -functionn to print numbers in order up to 98
 *@n: number to start from
 */
void print_to_98(int n)
{
  if (n < 98)
    {
      while(n < 98)
	{
	  printf("%d", n);
	  if(n != 98)
	    printf(", ");
	  n++;
	}
    }
  else if(n > 98)
    {
      while(n > 98)
	{
	  printf("%d", n);
	  if(n != 98)
	    printf(", ");
	  n--;
	}
}
else
{
printf("98");
}
}
